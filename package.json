{
  "name": "fetch-json-cache",
  "version": "1.2.0",
  "description": "Caches fetched json. Updates when etag changes and is uses cache regardless if endpoint unreachable. Uses write-file-atomic for safe updates",
  "keywords": [
    "every",
    "async",
    "array",
    "callback"
  ],
  "homepage": "https://github.com/kmalakoff/fetch-json-cache",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/fetch-json-cache.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "core-js": "^3.39.0",
    "get-remote": "^1.2.2",
    "mkdirp-classic": "^0.5.3",
    "rimraf2": "^2.8.2",
    "write-file-atomic": "^1.3.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^14.18.63",
    "depcheck": "^1.4.7",
    "fs-access-sync-compat": "^1.0.2",
    "ts-dev-stack": "^1.4.0"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts",
    "targets": [
      "cjs",
      "esm"
    ]
  }
}
