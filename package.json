{
  "name": "fetch-json-cache",
  "version": "1.2.12",
  "description": "Caches fetched json. Updates when etag changes and is uses cache regardless if endpoint unreachable. Uses write-file-atomic for safe updates",
  "keywords": [
    "every",
    "async",
    "array",
    "callback"
  ],
  "homepage": "https://github.com/kmalakoff/fetch-json-cache",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/fetch-json-cache.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.cjs",
    "types": "./dist/types/index.d.ts"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "get-remote": "^1.3.10",
    "mkdirp-classic": "^0.5.3",
    "queue-cb": "^1.4.11",
    "rimraf2": "^2.8.2",
    "temp-suffix": "^0.1.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5",
    "fs-exists-sync": "^0.1.0",
    "pinkie-promise": "^2.0.1"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
